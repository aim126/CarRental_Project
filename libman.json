{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "twitter-bootstrap@4.5.0",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "chat.js@1.0.2",
      "destination": "wwwroot/js/signalr/chat.js/",
      "files": [
        "src/chat.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "chat.js@1.0.2",
      "destination": "wwwroot/js/signalr/chat.js/",
      "files": [
        "dist/chat.js"
      ]
    },
    {
      "library": "@microsoft/signalr@3.1.4",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/@js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}